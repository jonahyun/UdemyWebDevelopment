<%- include("header") -%>
<div class="box" id="heading">
  <h1><%= listTitle %></h1>
</div>
<div class="box">
  
    <% for (var i=0; i < newTasks.length; i++) { %>
    <div class="item">
      <p><%= newTasks[i] %></p>
    </div>
    <% }; %>
    

  <form action="/" method="post">
    <input type="text" name="newTask" />
    <button type="submit" name="button" value="<%= listTitle %>">+</button> 
  </form>

  </div>
  <%- include("footer") -%>